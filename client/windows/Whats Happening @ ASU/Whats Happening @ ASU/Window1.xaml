<Window x:Class="Whats_Happening___ASU.Window1"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="What's Happening @ ASU" Height="600" Width="800">
    <Grid>
    <Grid.ColumnDefinitions>
        <ColumnDefinition />
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition MinWidth="70" />
    </Grid.ColumnDefinitions>
    <DockPanel Grid.Column="0" Grid.ColumnSpan="1" HorizontalAlignment="Stretch" Margin="0" Background="Maroon" Name="Map">
    </DockPanel>
    <GridSplitter Grid.Column="1" Grid.ColumnSpan="1" ResizeDirection="Columns" Height="Auto" Width="10" VerticalAlignment="Stretch" Margin="0" Name="Split" />
    <Grid Grid.Column="2" VerticalAlignment="Stretch" Margin="0" Background="Gold" Name="Results">
    </Grid>
</Grid>
</Window>
